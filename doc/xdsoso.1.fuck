<script language="javascript">
function chkinput(form){
	if(form.netid.value == "")
	{
		alert("学号不能为空！");
		form.netid.select();
		return(false);
	}
	if(form.password.value == "")
	{
		alert("密码不能为空！");
		form.netid.select();
		return(false);
	}	
	var _idreg = new RegExp("^[0-9]{6,12}$");
	if(!_idreg.test(form.netid.value))
	{
		alert("学号不正确，为6-12个数字！");
		form.netid.select();
		return(false);
	}	
	var _pwdreg = new RegExp("^.{4,16}$");
	if(!_pwdreg.test(form.password.value))
	{
		alert("密码不正确，为4-16个字符！");
		form.netid.select();
		return(false);
	}
	return(true);		
}
</script>
