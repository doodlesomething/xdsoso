            urlProtocol1 = uc1.urlProtocol + "://";
            urlPort1 = "" === uc1.urlPort ? "" : (":" + uc1.urlPort);
        }
        uc2.setUrl(urlProtocol1 + uc1.urlHostname + urlPort1 + uc1.getPath());
        switch (parseInt(loginSsl)) {
            case 1:
